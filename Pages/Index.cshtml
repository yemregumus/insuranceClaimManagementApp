@page
@model InsuranceClaimManagement.Pages.IndexModel
@{
    ViewData["Title"] = "Dashboard";
}

<h1 style="text-align: center;">Insurance Claim Management App Dashboard</h1>
<br />
<br />
<br />
<br />
<div>
    <h2>Users List</h2>
    <table class="table">
        <thead>
            <tr>
                <th>User ID</th>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
        
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model.Users)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Name</td>
                    <td>@user.Username</td>
                    <td>@user.Email</td>

                </tr>
            }
        </tbody>
    </table>
</div>
<br />
<br />
<br />
<br />
<div>
    <h2>Claims List</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Claim ID</th>
                <th>Claim Type</th>
                <th>Description</th>
                <th>Status</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var claim in Model.Claims)
            {
                <tr>
                    <td>@claim.Id</td>
                    <td>@claim.ClaimType</td>
                    <td>@claim.Description</td>
                    <td>@claim.Status</td>

                </tr>
            }
        </tbody>
    </table>
</div>
